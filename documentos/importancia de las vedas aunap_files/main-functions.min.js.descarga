$(function(){$(".min-fontsize").click(function(){var o=$("body").css("font-size"),s=parseFloat(o,10)-1;s>=16&&$("body, html").css("font-size",s)}),$(".max-fontsize").click(function(){var o=$("body").css("font-size"),s=parseFloat(o,10)+1;s<=22&&$("body, html").css("font-size",s)}),$(".contrast-ref").click(function(){return $("body").hasClass("configHighContrastTools")?$("body").removeClass("configHighContrastTools"):$("body").addClass("configHighContrastTools"),!1})});